//
// Definition of lexemes in vector.
// These are defined in a tree structure that
// groups tokens of the same class together

keywords:
   DEF:        "def"
   VAR:        "var"
   IF:         "if"
   ELSE:       "else"
   TYPE:       "type"
   STRUCT:     "struct"
   TYPEDEF:    "typedef"
   SWITCH:     "switch"
   CASE:       "case"
   FOR:        "for"
   WHILE:      "while"
   ENDIF:      "endif"
   ENDFOR:     "endfor"
   ENDWHILE:   "endwhile"
   ENDSWITCH:  "endswitch"
   END:        "end"

symbols:
   AT:   "@"
   TERM: ";"

ops:
   logic_ops:
      LT:   "<"
      GT:   ">"
      EQ:   "=="
      LTE:  "<="
      GTE:  ">="
   arith_ops:
      ADD:      "+"
      SUB:      "-"
      DIV:      "/"
      MULT:     "*"
      INC:      "+="
      DEC:      "-="
      MULTINC:  "*="
      DIVDEC:   "/="
   bitwise_ops:
      // should be implemented in a library instead
   extra_ops:
      // implemented by the type objects to add syntactic sugar to some operations
literals:

   