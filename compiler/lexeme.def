//
// Definition of lexemes in vector.
// These are defined in a tree structure that
// groups tokens of the same class together

keywords:
   DEF:        "def"
   IF:         "if"
   ELSE:       "else"
   TYPE:       "type"
   STRUCT:     "struct"
   TYPEDEF:    "typedef"
   SWITCH:     "switch"
   CASE:       "case"
   FOR:        "for"
   WHILE:      "while"
   ENDIF:      "endif"
   ENDFOR:     "endfor"
   ENDWHILE:   "endwhile"
   ENDSWITCH:  "endswitch"
   END:        "end"
   RETURN:     "return"
   YIELD:      "yield"
   CONST:      "const"
   NEW:        "new"
symbols:
   AT:    "@"
   TERM:  ";"
   LSB:   "["
   RSB:   "]"
   LCB:   "{"
   RCB:   "}"
   COL:   ":"
   COM:   "//"
   LBr:   "("
   RBr:   ")"
   ARRW:  "->" 
ops:
   logic_ops:
      LT:   "<"
      GT:   ">"
      EQ:   "=="
      NEQ:  "!="
      LTE:  "<="
      GTE:  ">="
   arith_ops:
      ADD:      "+"
      SUB:      "-"
      DIV:      "/"
      MULT:     "*"
      INC:      "+="
      DEC:      "-="
      MULTINC:  "*="
      DIVDEC:   "/="
   bitwise_ops:
      // should be implemented in a library instead
   extra_ops:
      // implemented by the type objects to add syntactic sugar to some operations
literals:
   char:"a" ... "z"| "A" ... "Z"
   num: 0 ... 9
   us: "_"